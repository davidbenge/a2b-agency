# =============================================================================
# AGENCY TO BRAND (.env) CONFIGURATION FILE
# =============================================================================
# 
# This file contains all the environment variables needed to run the a2b-agency
# application. Copy this file to .env and replace the fake values with your
# actual credentials from Adobe Developer Console and AEM Cloud Service.
#
# IMPORTANT: This file must NOT be committed to source control!
# =============================================================================

# =============================================================================
# ADOBE I/O RUNTIME CONFIGURATION
# =============================================================================
# These values are automatically generated when you run 'aio app use'
# and should not be changed manually.

# Runtime authentication key (format: key:secret)
AIO_runtime_auth=97d7c0f6-4171-4453-bbe3-123456789abc:abcdef1234567890abcdef1234567890abcdef12

# Runtime namespace (format: consoleId-projectName-workspace)
AIO_runtime_namespace=27200-a2b-agency-dev

# Runtime API host (usually doesn't change)
AIO_runtime_apihost=https://adobeioruntime.net

SERVICE_API_KEY=cm-1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
AIO_ims_contexts_Credential__in__agency__to__brand__-__benge_client__id=cm-1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
AIO_ims_contexts_Credential__in__agency__to__brand__-__benge_client__secrets=["p8e-1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef"]
AIO_ims_contexts_Credential__in__agency__to__brand__-__benge_technical__account__email=FAKE-5c60-4ed7-a0e8-b2d6d498f46e@techacct.adobe.com
AIO_ims_contexts_Credential__in__agency__to__brand__-__benge_technical__account__id=FAKE@techacct.adobe.com
AIO_ims_contexts_Credential__in__agency__to__brand__-__benge_scopes=["AdobeID","openid","read_organizations","additional_info.projectedProductContext","additional_info.roles","adobeio_api","read_client_secret","manage_client_secrets"]
