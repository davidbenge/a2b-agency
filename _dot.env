###### do not change the items in your .env above this. It comes from your IO workspace

# =============================================================================
# ADOBE DEVELOPER CONSOLE - SERVER TO SERVER CREDENTIALS
# =============================================================================
# These values come from your Adobe Developer Console project's Service Account

# Organization ID from your Adobe Developer Console
ORG_ID=27200ABCDEF1234567890@AdobeOrg

# Service Account API Key (also called Client ID)
S2S_API_KEY=cm-1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Service Account Client Secret
S2S_CLIENT_SECRET=p8e-1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Service Account Scopes (usually don't change)
S2S_SCOPES=["AdobeID","openid","read_organizations","additional_info.projectedProductContext","additional_info.roles","adobeio_api","read_client_secret","manage_client_secrets"]

# =============================================================================
# ADOBE I/O EVENTS - EVENT PROVIDER IDs
# =============================================================================
# These UUIDs come from your event providers in Adobe I/O Events
# Use 'aio event provider list' to find existing providers
# Use 'aio event provider create' to create new ones if needed

# Brand registration events provider ID
AIO_AGENCY_EVENTS_REGISTRATION_PROVIDER_ID=5c3431a2-bd91-4eff-a356-26b747d0aad4

# Asset sync events provider ID
AIO_AGENCY_EVENTS_AEM_ASSET_SYNC_PROVIDER_ID=af5c4d93-e1e0-4985-ad33-80fde3837aaa

# =============================================================================
# AEM CLOUD SERVICE DEVELOPER CONSOLE - JWT INTEGRATION
# =============================================================================
# These values come from your AEM Cloud Service Developer Console

# AEM JWT Client ID
AEM_AUTH_CLIENT_ID=cm-1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# AEM Technical Account ID
AEM_AUTH_TECH_ACCOUNT_ID=B150222D68FAKE495EF3@techacct.adobe.com

# AEM Client Secret
AEM_AUTH_CLIENT_SECRET=p8e-3ly5DXFAKE3vguN0J2U4YBSXb

# AEM Authentication Scopes (usually don't change)
AEM_AUTH_SCOPES=ent_aem_cloud_api

# AEM Authentication Type (usually jwt)
AEM_AUTH_TYPE=jwt

# AEM Private Key - IMPORTANT: Special formatting required!
# 1. Download JWT token from AEM Developer Console
# 2. Remove the last \r\n from the key
# 3. Replace all other \r\n with \n
# 4. Surround the entire key with double quotes
AEM_AUTH_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\nFAKE\n-----END RSA PRIVATE KEY-----"

# =============================================================================
# APPLICATION-SPECIFIC CONFIGURATION
# =============================================================================
# These values are specific to your agency and can be customized

# Application name
AIO_app_name=a2b-agency

# info used to help with event filtering
APPLICATION_RUNTIME_INFO: '{"namespace":"${AIO_runtime_namespace}","app_name":"brand","action_package_name":"${AIO_ACTION_PACKAGE_NAME}"}'


# Your agency name (used in events and logging)
AGENCY_NAME=Benge Agency

# Unique identifier for your agency (generate a UUID) 
# Each agency should have their own value. This will be appended to events for validation and filtering
AGENCY_ID=2ff22120-d393-fake-afdd-0d4b2038d2be

# =============================================================================
# ADOBE INTERNAL CALLS CONFIGURATION
# =============================================================================
# URL for Adobe internal calls

# Adobe internal calls endpoint
ADOBE_INTERNAL_URL_ENDPOINT=https://27200-609silverstork-stage.adobeioruntime.net/api/v1/web/a2b-agency

# =============================================================================
# OPTIONAL - MONITORING & ANALYTICS
# =============================================================================
# These are optional
# This is used for debugging and you should go sign up for an account and see this page for how to setup  
# https://developer.adobe.com/app-builder/docs/guides/app_builder_guides/application_logging/new-relic

# New Relic license key (optional - for monitoring)
NEW_RELIC__LICENSE_KEY=873BENGEa77eFAKE4fa5533FFFFNRAL

AIO_ENABLE_DEMO_MODE=false
